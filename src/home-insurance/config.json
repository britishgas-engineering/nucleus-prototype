{
    "name" : "home-insurance",
    "title" : "Home Insurance",
    "template" : "multi-page-form",
    "pages": [{
            "name": "intro",
            "title": "What is the postcode of your home?",
            "description": "This is the address of the home your want to insure.",
            "fields": [{
                    "name": "address",
                    "label": "Your Address",
                    "type": "address",
                    "validation": "[\"isRequired\"]"
                }
            ]
        },
        {
            "name": "ownership",
            "title": "Do you own or rent your home?",
            "description": "",
            "fields": [
                {
                    "name": "ownership",
                    "label": "Do you own your own home?",
                    "type": "radio",
                    "options": "['Own', 'Mortgage', 'Rent']",
                    "validation": "[\"isRequired\"]"
                }
            ]
        },
        {
            "name": "start-date",
            "title": "Get a Home Insurance quote",
            "description": "",
            "fields": [
                {
                    "name": "start-date",
                    "label": "When would you like your cover to start?",
                    "type": "datePicker",
                    "validation": "[\"isRequired\",\"isDate\"]"
                }
            ]
        },
        {
            "name": "cover",
            "title": "Get a Home Insurance quote",
            "description": "",
            "fields": [
                {
                    "name": "cover-type",
                    "label": "What type of cover do you need?",
                    "type": "radio",
                    "options": "['Buildings and contents', 'Contents', 'Building']",
                    "validation": "[\"isRequired\"]"
                }
            ]
        },
        {
            "name": "personal",
            "title": "Your personal details",
            "description": "",
            "fields": [{
                "name": "title",
                "label": "Title",
                "type": "select",
                "options": "['Mr', 'Mrs']",
                "validation": "[\"isRequired\"]"
            },{
                "name": "first-name",
                "label": "First Name",
                "type": "text",
                "validation": "[\"isRequired\"]"
            },{
                "name": "surname",
                "label": "Surname",
                "type": "text",
                "validation": "[\"isRequired\"]"
            },{
                "name": "email",
                "label": "Email",
                "type": "text",
                "validation": "[\"isRequired\", \"isEmail\"]"
            },
            {
                "name": "phone",
                "label": "Phone",
                "type": "text",
                "validation": "[\"isRequired\"]"
            },
            {
                "name": "dob",
                "label": "Date of birth",
                "type": "text",
                "mask": "dd/mm/yyyy",
                "separator": "/",
                "validation": "[\"isRequired\",\"isDate\"]"
            }
            ]
        },
        {
            "name": "cover",
            "title": "",
            "description": "",
            "fields": [
                {
                    "name": "cover-type",
                    "label": "How would you like to receive your policy documents?",
                    "type": "radio",
                    "options": "['Email', 'Post']",
                    "validation": "[\"isRequired\"]"
                }
            ]
        }
    ]
}