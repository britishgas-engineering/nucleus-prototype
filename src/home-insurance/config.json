{
    "name" : "home-insurance",
    "title" : "Home Insurance",
    "template" : "multi-page-form",
    "pages": [{
            "name": "intro",
            "title": "Your address",
            "description": "Please give us the address of the home you want to insure.",
            "fields": [{
                    "name": "address",
                    "label": "Your Address",
                    "type": "address",
                    "validation": "[\"isRequired\"]"
                }
            ]
        },
        {
            "name": "ownership",
            "title": "Your home",
            "description": "",
            "fields": [
                {
                    "name": "ownership",
                    "label": "Do you own or rent your home?",
                    "type": "radio",
                    "options": "['Own', 'Mortgage', 'Rent']",
                    "validation": "[\"isRequired\"]"
                }
            ]
        },
        {
            "name": "start-date",
            "title": "Starting your cover",
            "description": "",
            "fields": [
                {
                    "type": "highlighter",
                    "icon": "info",
                    "heading": "About your start date",
                    "message": "You can start your insurance cover any time within the next 60 days. You cant backdate the start of your policy."
                },
                {
                    "name": "start-date",
                    "label": "What date would you like your cover to start?",
                    "type": "datePicker",
                    "validation": "[\"isRequired\",\"isDate\"]"
                }
            ]
        },
        {
            "name": "cover",
            "title": "Your cover",
            "description": "",
            "fields": [
                {
                    "name": "cover-type",
                    "label": "What type of cover do you need?",
                    "type": "radio",
                    "options": "['Buildings and contents', 'Contents', 'Buildings']",
                    "validation": "[\"isRequired\"]"
                }
            ]
        },
        {
            "name": "personal",
            "title": "Your personal details",
            "description": "",
            "fields": [{
                "name": "title",
                "label": "Title",
                "type": "select",
                "options": "['Mr', 'Mrs']",
                "validation": "[\"isRequired\"]"
            },{
                "name": "first-name",
                "label": "First Name",
                "type": "text",
                "validation": "[\"isRequired\"]"
            },{
                "name": "surname",
                "label": "Surname",
                "type": "text",
                "validation": "[\"isRequired\"]"
            },{
                "name": "email",
                "label": "Email",
                "type": "text",
                "validation": "[\"isRequired\", \"isEmail\"]"
            },
            {
                "name": "phone",
                "label": "Phone",
                "type": "text",
                "validation": "[\"isRequired\"]"
            },
            {
                "name": "dob",
                "label": "Date of birth",
                "type": "text",
                "mask": "dd/mm/yyyy",
                "separator": "/",
                "validation": "[\"isRequired\",\"isDate\"]"
            }
            ]
        }
    ]
}