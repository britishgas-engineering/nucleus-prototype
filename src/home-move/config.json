{
    "name" : "home-move",
    "title" : "Home Move",
    "template" : "multi-page-form",
    "pages": [{
            "name": "intro",
            "title": "Moving out date",
            "description": "We need to know these dates.",
            "fields": [
                {
                    "name": "move-date",
                    "label": "What date are you moving out?",
                    "type": "datePicker",
                    "validation": "[\"isRequired\"]"
                }
            ]
        },
        {
            "name": "meter-read",
            "title": "Move out meter readings",
            "description": "We’ll estimate any meter readings that you don’t fill in.",
            "fields": [
                {
                    "name": "gas-meter",
                    "label": "Gas meter reading",
                    "type": "text",
                    "mask": "00---",
                    "validation": "[\"isRequired\"]"
                },
                {
                    "name": "electricity-meter",
                    "label": "Electricity meter reading",
                    "type": "text",
                    "mask": "00---",
                    "validation": "[\"isRequired\"]"
                }
            ]
        },
        {
            "name": "address",
            "title": "What is your new address?",
            "description": "This is the address that you’d like to set up your products.",
            "fields": [
                {
                    "name": "address",
                    "label": "What is your new address?",
                    "type": "address",
                    "validation": "[\"isRequired\"]"
                }
            ]
        },
        {
            "name": "moveday",
            "title": "Moving Home",
            "description": "",
            "fields": [
                {
                    "name": "same-day",
                    "label": "Are you moving in and out on the same day?",
                    "type": "radio",
                    "options": "['Yes', 'No']",
                    "validation": "[\"isRequired\"]"
                }
            ]
        }]
}
