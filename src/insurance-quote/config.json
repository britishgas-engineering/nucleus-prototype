{
    "name" : "home-insurance",
    "title" : "Home Insurance",
    "template" : "multi-page-form",
    "pages": [{
            "name": "intro",
            "title": "Get a Home Insurance quote",
            "description": "",
            "fields": [{
                    "name": "address",
                    "label": "Your Address",
                    "type": "address",
                    "validation": "[\"isRequired\"]"
                }
            ]
        },
        {
            "name": "product",
            "title": "Get a Home Insurance quote",
            "description": "",
            "fields": [
                {
                    "name": "ownership",
                    "label": "Do you own your own home?",
                    "type": "radio",
                    "options": "['Yes', 'No']",
                    "validation": "[\"isRequired\"]"
                },
                {
                    "name": "start-date",
                    "label": "What date would you like the cover to start?",
                    "type": "datePicker",
                    "validation": "[\"isRequired\",\"isDate\"]"
                },
                {
                    "name": "cover-type",
                    "label": "What type of cover do you require?",
                    "type": "radio",
                    "options": "['Building Insurance only', 'Building and Contentd insurance']",
                    "validation": "[\"isRequired\"]"
                }
            ]
        },
        {
            "name": "policy",
            "title": "Get a Home Insurance quote",
            "description": "The address of the home you want to insure is",
            "fields": [
                {
                    "name": "policy-documents",
                    "label": "How would you like to receive your policy documents?",
                    "type": "radio",
                    "options": "['Mail', 'Email', 'Both']",
                    "validation": "[\"isRequired\"]"
                }
            ]
        },
        {
            "name": "personal",
            "title": "Your details",
            "description": "",
            "fields": [{
                "name": "title",
                "label": "Title",
                "type": "select",
                "options": "['Mr', 'Mrs']",
                "validation": "[\"isRequired\"]"
            },{
                "name": "first-name",
                "label": "First Name",
                "type": "text",
                "validation": "[\"isRequired\"]"
            },{
                "name": "surname",
                "label": "Surname",
                "type": "text",
                "validation": "[\"isRequired\"]"
            },{
                "name": "email",
                "label": "Email",
                "type": "text",
                "validation": "[\"isRequired\", \"isEmail\"]"
            },
            {
                "name": "phone",
                "label": "Phone",
                "type": "text",
                "validation": "[\"isRequired\"]"
            },
            {
                "name": "dob",
                "label": "Date of birth",
                "type": "text",
                "mask": "dd/mm/yyyy",
                "separator": "/",
                "validation": "[\"isRequired\",\"isDate\"]"
            }
            ]
        }
    ]
}