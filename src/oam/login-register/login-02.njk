---
title: Log in to your account
---

<ns-landmark type="lakeside">
  <h1 slot="heading">
    <span class="h5">My Account.</span>
    <span class="h1">Enter your password</span>
  </h1>
  <p slot="paragraph">Please enter the password for <strong>known@address.com</strong></p>
</ns-landmark>

<ns-panel>
  <div class="splash">
  <ns-form onsubmit="submittedForm(event)">
<ns-fieldset>
<ns-inputter mask="" separator="" validation="[&quot;isRequired&quot;]" value="" helper="" name="password">

  <label slot="label" for="password-input">Password</label>
  <input type="password" id="password-input">
</ns-inputter></ns-fieldset>

<ns-cta nf-href="DYNAMIC">Log in to my account</ns-cta>

<ns-cta type="text" href="../magic-01" icon="arrow-right" loading="false" loadingmessage="Loading...">Email me a login link</ns-cta>



    </ns-form>

</div>
</ns-panel>


{% include "./_includes/ns-accordion-login.njk" -%}


<script>
  function getDynamicURL() {
    var data = document.querySelector('ns-inputter[name="password"]').value;
    if (data === 'incorrect') {
      return '../login-03-fail';
    } else {
      return '../login-04-success';
    }
  }
</script>
